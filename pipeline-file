pipeline {
    agent any
    stages {
        stage('git-checkout') {
            steps {
               git 'https://github.com/PrasenjeetW/repo1.git'
            }
        }
        stage('mvn-build') {
            steps {
                sh 'sudo apt-get update -y'
                sh 'sudo apt install maven -y'
                sh 'mvn clean package'
            }
        }
    }
}
